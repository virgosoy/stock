{
    "manifest_version": 2,
    "name": "stock",
    "description": "股票插件",
    "version": "0.0.0.10",
    "browser_action": {
        "default_title": "股票插件",
        "default_popup": "popup.html",
        "default_icon": "image/icon48.png"
    },
    "icons": {
        "16": "image/icon48.png"
    },
    "background": {
        "scripts": [
        	"js/lodash/lodash-4.17.15.js",
            "js/utils.js",
        	"js/background.js"
        ]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/content.js"]
        // "all_frames": true
    }],
    // 权限申请
    "permissions": [
        "storage", // 存储
    	"notifications", // 通知
    	"http://hq.sinajs.cn/*"
    ],
    // 可选，向地址栏注册一个关键字以提供搜索建议，只能设置一个关键字
    "omnibox": { "keyword" : "stock" }
}